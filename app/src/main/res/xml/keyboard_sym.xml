            android:keyWidth="10.5%p" />
        <Key
            android:codes="-22"
            android:keyLabel="TERM"
            android:keyWidth="11.5%p" />
        <Key
            android:codes="-23"
            android:keyLabel="SUPER"
            android:keyWidth="11.5%p" />
        <Key
            android:codes="-24"
            android:keyEdgeFlags="right"
            android:keyLabel="SUB"
            android:keyWidth="11.5%p" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="-3"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/keyboard_close"
            android:keyWidth="15%p" />
        <Key
            android:codes="-6"
            android:keyLabel="ABC"
            android:keyWidth="15%p" />
        <Key
            android:codes="-5"
            android:keyIcon="@drawable/ic_keyboard_capslock_black_24dp"
            android:keyWidth="15%p" />
        <Key
            android:codes="-16"
            android:keyLabel="HELP" />
        <Key
            android:codes="-11"
            android:keyLabel="BACK"
            android:keyWidth="25%p" />
        <Key
            android:codes="66"
            android:keyEdgeFlags="right"
            android:keyLabel="NEXT"
            android:keyWidth="25%p" />
