<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="8%p"
    android:keyWidth="10%p"
    android:verticalGap="0px">
    <Row
        android:keyWidth="9%p"
        android:rowEdgeFlags="top">
        <Key
            android:codes="-30"
            android:keyEdgeFlags="left"
            android:keyLabel="&lt;" />
        <Key
            android:codes="-31"
            android:keyLabel="&gt;" />
        <Key
            android:codes="-32"
            android:keyLabel="[" />
        <Key
            android:codes="-33"
            android:keyLabel="]" />
        <Key
            android:codes="-34"
            android:keyLabel="$" />
        <Key
            android:codes="-35"
            android:keyLabel="%" />
        <Key
            android:codes="-36"
            android:keyLabel="_" />
        <Key
            android:codes="-37"
            android:keyLabel="'" />
        <Key
            android:codes="-38"
            android:keyLabel="*" />
        <Key
            android:codes="-39"
            android:keyLabel="(" />
        <Key
            android:codes="-40"
            android:keyEdgeFlags="right"
            android:keyLabel=")" />
    </Row>

    <Row android:keyWidth="9%p">
        <Key
            android:codes="-41"
            android:keyEdgeFlags="left"
            android:keyLabel="\u0040" />
        <Key
            android:codes="-42"
            android:keyLabel="#" />
        <Key
            android:codes="-43"
