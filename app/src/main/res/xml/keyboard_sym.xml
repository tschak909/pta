        <Key
            android:codes="-47"
            android:keyLabel="\u005c" />
        <Key
            android:codes="-48"
            android:keyLabel="|" />
        <Key
            android:codes="-57"
            android:keyLabel="\u03a3" />
        <Key
            android:codes="55"
            android:keyLabel="," />
        <Key
            android:codes="56"
            android:keyEdgeFlags="right"
            android:keyLabel="." />
    </Row>

    <Row>
        <Key
            android:codes="-51"
            android:keyLabel="TAB" />
        <Key
            android:codes="-52"
            android:keyLabel="\u00F7" />
        <Key
            android:codes="-53"
            android:keyLabel="-" />
        <Key
            android:codes="-54"
            android:keyLabel="+" />
        <Key
            android:codes="-10"
            android:keyLabel="ANS"
            android:keyWidth="11.5%p" />
        <Key
            android:codes="-12"
